{
  "name": "@volusion/element-cli",
  "version": "3.1.1",
  "description": "Command line interface for the Volusion Element ecosystem",
  "author": "Volusion, LLC",
  "main": "bin/src/index.js",
  "license": "MIT",
  "homepage": "https://github.com/volusion/element-cli#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/volusion/element-cli.git"
  },
  "private": false,
  "preferGlobal": true,
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "element": "./bin/src/index.js"
  },
  "resolutions": {
    "**/**/node-gyp": "5.0.0"
  },
  "dependencies": {
    "axios": "^0.18.1",
    "chalk": "^2.4.1",
    "commander": "^2.15.1",
    "inquirer": "^6.2.0",
    "replace-in-file": "^3.4.0",
    "rimraf": "^2.6.2",
    "uglify-js": "^3.6.0"
  },
  "devDependencies": {
    "@types/inquirer": "0.0.43",
    "@types/jest": "^25.1.4",
    "@types/node": "^11.11.1",
    "@types/nodegit": "^0.24.4",
    "@types/rimraf": "^2.0.2",
    "@types/uglify-js": "^3.0.4",
    "audit-ci": "^3.0.0",
    "husky": "^0.14.3",
    "jest": "^25.1.0",
    "mermaid.cli": "^0.5.1",
    "prettier": "^1.14.0",
    "ts-jest": "^25.5.1",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.14.0",
    "tslint-microsoft-contrib": "^6.0.0",
    "tslint-no-focused-test": "^0.5.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^3.8.3"
  },
  "scripts": {
    "audit:ci": "audit-ci --high",
    "build": "tsc --pretty",
    "build:watch": "tsc --watch --pretty",
    "doc": "mmdc -i docs/*.mmd",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "lint:fix": "tslint -c tslint.json 'src/**/*.ts' --fix",
    "prepare": "npm run prepush",
    "prepush": "npm run lint && npm test",
    "watch": "tsc --watch --pretty",
    "test": "npm run build && npm run test:unit",
    "test:cov": "jest --coverage",
    "test:unit": "jest",
    "test:watch": "jest --watch"
  }
}
